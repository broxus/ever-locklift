{
  "name": "locklift",
  "version": "2.0.2",
  "description": "Node JS framework for working with Ever contracts. Inspired by Truffle and Hardhat. Helps you to build, test, run and maintain your smart contracts.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev-deploy-local": "npm run build && npm publish --registry http://localhost:4873/",
    "dev-deploy": "npm run build && npm publish --registry http://95.216.205.173:4873/"
  },
  "bin": {
    "locklift": "build/cli/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "build/index.d.ts"
      ],
      "config": [
        "build/config/index.d.ts"
      ],
      "contract": [
        "build/contract/index.d.ts"
      ],
      "factory": [
        "build/factory/index.d.ts"
      ],
      "giver": [
        "build/giver/index.d.ts"
      ],
      "utils": [
        "build/utils/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broxus/ever-locklift.git"
  },
  "keywords": [
    "Everscale",
    "TON",
    "blockchain",
    "smart-contracts",
    "javascript",
    "solidity",
    "dapps",
    "ethereum"
  ],
  "author": "pavlovdog_ <pavlovdog.moscow@gmail.com>",
  "contributors": [
    "30mb1 <steam.30mb1@gmail.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/broxus/ever-locklift/issues"
  },
  "homepage": "https://github.com/broxus/ever-locklift#readme",
  "dependencies": {
    "@tonclient/core": "^1.28.0",
    "@tonclient/lib-node": "^1.28.0",
    "@types/download": "^8.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node-gzip": "^1.1.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.1",
    "commander": "^7.2.0",
    "cross-env": "^7.0.3",
    "directory-tree": "^2.2.7",
    "download": "^8.0.0",
    "ejs": "^3.1.6",
    "env-paths": "^2.2.1",
    "everscale-crypto": "^0.1.1",
    "everscale-inpage-provider": "^0.3.26",
    "everscale-standalone-client": "^2.0.16",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.21",
    "mocha": "^8.3.2",
    "node-gzip": "^1.1.2",
    "prettier": "^2.7.1",
    "rxjs": "^7.5.6",
    "superstruct": "^0.14.2",
    "tablemark": "^2.0.0",
    "ts-mocha": "^10.0.0",
