    "@types/chai": "^4.3.1",
    "@types/ejs": "^3.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.45",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "^4.7.4"
  }
